{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Elasticsearch",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "MVC": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": { "path": "MVC_log.txt" }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://10.0.75.1:9200",
          "autoRegisterTemplate": true,
          "minimumLogEventLevel": "Information"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "RedisConnectionString": "localhost",
  "IdentityUrl": "http://localhost:5105/",
  "CallBackUrl": "http://localhost:5100/",
  //
  // CATALOG
  //
  "CatalogConnectionString": "Server=(localdb)\\mssqllocaldb;Database=Catalog;Trusted_Connection=True;MultipleActiveResultSets=true",
  //
  // ORDERING
  //
  "OrderingConnectionString": "Server=(localdb)\\mssqllocaldb;Database=Ordering;Trusted_Connection=True;MultipleActiveResultSets=true"
}
